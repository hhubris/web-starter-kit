{
  "name": "web-starter-kit",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "bootstrap": "3.3.7",
    "hapi": "17.2.0",
    "inert": "5.0.1",
    "lodash": "4.17.4",
    "react": "16.2.0",
    "react-dom": "16.2.0",
    "react-redux": "5.0.6",
    "react-router-dom": "4.2.2",
    "react-scripts": "1.0.17",
    "redux": "3.7.2",
    "redux-devtools": "3.4.1",
    "redux-mock-store": "1.3.0",
    "whatwg-fetch": "2.0.3",
    "winston": "2.4.0"
  },
  "scripts": {
    "build": "react-scripts build",
    "eject": "react-scripts eject",
    "lint": "npm run lint:js",
    "lint:eslint": "eslint --ignore-path .gitignore",
    "lint:js": "npm run lint:eslint -- . ",
    "server:debug": "inspect server",
    "server": "node server",
    "start": "react-scripts start",
    "test": "npm run lint && npm run test:client:coverage && npm run test:server:coverage",
    "test:client": "react-scripts test --env=jsdom",
    "test:client:coverage": "npm run test:client -- --coverage",
    "test:client:debug": "react-scripts --inspect-brk test --runInBand --env=jsdom",
    "test:server": "mocha --watch server/test",
    "test:server:coverage": "nyc --reporter text --reporter html --report-dir coverage-server mocha server/test"
  },
  "devDependencies": {
    "chai": "4.1.2",
    "cross-env": "5.1.3",
    "enzyme": "3.2.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "eslint": "4.14.0",
    "eslint-config-google": "0.9.1",
    "eslint-plugin-react": "7.5.1",
    "fetch-mock": "5.13.1",
    "inspect-process": "0.5.0",
    "jest-cli": "22.0.4",
    "mocha": "4.0.1",
    "nyc": "11.4.1",
    "react-test-renderer": "16.2.0",
    "sinon": "4.1.3"
  }
}
